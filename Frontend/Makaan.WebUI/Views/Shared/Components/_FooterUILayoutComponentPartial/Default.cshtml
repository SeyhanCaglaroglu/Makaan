@model List<ResultContactDto>
<div class="container-fluid bg-dark text-white-50 footer pt-5 mt-5 wow fadeIn" data-wow-delay="0.1s">
    <div class="container py-5">
        <div class="row g-5">
            <div class="col-lg-3 col-md-6">
                <h5 class="text-white mb-4">İletişim Kanalları</h5>
                @foreach (var item in Model)
                {
                    <p class="mb-2"><i class="fa fa-map-marker-alt me-3"></i>@item.Location, @item.City, @item.Country</p>
                    <p class="mb-2"><i class="fa fa-phone-alt me-3"></i>@item.Phone</p>
                    <p class="mb-2"><i class="fa fa-envelope me-3"></i>@item.Email</p>
                }
                
                <div class="d-flex pt-2">
                    <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-twitter"></i></a>
                    <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-facebook-f"></i></a>
                    <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-youtube"></i></a>
                    <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-linkedin-in"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-md-6">
                <h5 class="text-white mb-4">Sayfa Linkleri</h5>
                <a class="btn btn-link text-white-50" href="/About/Index">Hakkımızda</a>
                <a class="btn btn-link text-white-50" href="/Contact/Index">İletişim</a>
                <a class="btn btn-link text-white-50" href="/Property/Index">Mülkler</a>
                <a class="btn btn-link text-white-50" href="/Comment/Index">Yorumlar</a>
                <a class="btn btn-link text-white-50" href="/Account/Login">Giriş Yap</a>
            </div>
            <div class="col-lg-3 col-md-6">
                <h5 class="text-white mb-4">Fotoğraf Galerisi</h5>
                <div class="row g-2 pt-2">
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-1.jpg" alt="">
                    </div>
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-2.jpg" alt="">
                    </div>
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-3.jpg" alt="">
                    </div>
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-4.jpg" alt="">
                    </div>
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-5.jpg" alt="">
                    </div>
                    <div class="col-4">
                        <img class="img-fluid rounded bg-light p-1" src="~/SiteLayout/img/property-6.jpg" alt="">
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6">
                <h5 class="text-white mb-4">Haberler</h5>
                <p>Gelişmelerden Haberdar olmak için takipde kal.</p>
                <div class="position-relative mx-auto" style="max-width: 400px;">
                    <input class="form-control bg-transparent w-100 py-3 ps-4 pe-5" type="text" placeholder="Your email">
                    <button type="button" class="btn btn-primary py-2 position-absolute top-0 end-0 mt-2 me-2">Abone Ol</button>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="copyright">
            <div class="row">
                
                <div class="col-md-12 text-center text-md-end">
                    <div class="footer-menu">
                        <a href="/Default/Index">Anasayfa</a>
                        <a href="">Cookiler</a>
                        <a href="">Yardım</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
